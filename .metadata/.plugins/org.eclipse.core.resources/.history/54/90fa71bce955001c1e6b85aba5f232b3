package com.javacourse.intro;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class GcdAndLcmTest {
	
	@Test
	public void testGcd() {
		GcdAndLcm calculator = new GcdAndLcm();
		
		assertEquals("testing with 18 and 48", 5, calculator.gcd(18, 48));
		
	}

	@Test
	public void testLcd() {
		GcdAndLcm calculator = new GcdAndLcm();
		
		assertEquals(144, calculator.lcm(18, 48));
		
	}
}
