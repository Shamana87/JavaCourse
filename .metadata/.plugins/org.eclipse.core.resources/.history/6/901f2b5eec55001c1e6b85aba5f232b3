package com.javacourse.intro;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

public class Run {

	public static void main(String[] args) {
		
		final Logger logger = LoggerFactory.getLogger(GcdAndLcm.class);
		
		GcdAndLcm calculator = new GcdAndLcm();
		
		logger.debug("The greatest common devider is: " + calculator.gcd(18, 48));
		logger.debug("Least common multiple is: " + calculator.lcm(18, 48));


	}

}
